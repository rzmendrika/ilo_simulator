{% extends 'base/layout/pagelayout.html.twig' %}

{% block title %}G6K - {{ step.simulator.label}}{% endblock %}

{% block metas %}
{{ parent() }}<meta name="viewport" content="width=device-width, initial-scale=1.0" />
{% endblock %}

{% block styles %}
{{ parent() }}
<link rel='stylesheet' type='text/css' href="{{ asset('assets/Default/css/default.css') }}" />
<link rel='stylesheet' type='text/css' href="{{ asset('assets/base/js/libs/bootstrap/css/bootstrap.css') }}" />
{% endblock %}

{% block body %}
	{# {% include 'Default/sections/header.html.twig' %} #}
	<main class="main" id="main" role="main">
		<div class="container">
			{% block content %}{% endblock %}
		</div>
	</main>
	{# {% include 'Default/sections/footer.html.twig' %} #}
	{% block scripts %}
		{{ parent() }}
		<script type="text/javascript">
		$(function(){
			setTimeout(function() {
				$('input.date').datepicker('theme', 'blue');
				$('input.listbox-input').listbox('theme', 'blue');	
			}, 0);
		});
		</script>
	{% endblock %}
{% endblock %}
